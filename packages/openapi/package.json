{
  "name": "@repo/openapi",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "./dist/index.d.cts",
  "main": "./dist/index.cjs",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc",
    "codegen": "tsup && vite-node ./src/generate.ts"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@turbo/gen": "^1.10.12",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.5.2",
    "eslint": "^8.53.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite-node": "^1.0.2"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "zod": "^3.22.4"
  }
}
